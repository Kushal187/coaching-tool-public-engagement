{
  "name": "coaching-tool-public-engagement",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "dev": "node server.mjs & vite",
    "dev:server": "node server.mjs",
    "dev:vite": "vite",
    "start": "node server.mjs",
    "build": "vite build",
    "preview": "npm run build && node server.mjs",
    "convert": "python scripts/convert-pdfs.py",
    "convert:force": "python scripts/convert-pdfs.py --force",
    "ingest": "node scripts/ingest-pdfs.mjs",
    "ingest:clear": "node scripts/ingest-pdfs.mjs --clear",
    "ingest:simple": "node scripts/ingest-pdfs.mjs --simple",
    "pipeline": "npm run convert && npm run ingest",
    "pipeline:clear": "npm run convert && npm run ingest:clear",
    "ingest:excel": "python scripts/ingest.py",
    "ingest:excel:clear": "python scripts/ingest.py --clear",
    "ingest:excel:dry": "python scripts/ingest.py --dry-run",
    "ingest:excel:cs-only": "python scripts/ingest.py --only-case-study-library",
    "ingest:excel:skip-cs": "python scripts/ingest.py --skip-case-study-library"
  },
